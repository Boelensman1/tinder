{
  "name": "tinder",
  "version": "0.0.1",
  "description": "Wrapper for tinder",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint './src/**/*.ts' --project tsconfig.json --type-check",
    "clean": "rimraf lib",
    "prepare": "npm run lint && npm run clean && npm run compile"
  },
  "author": "Wigger Boelens <me@wiggerboelens.com> (http://wiggerboelens.com/)",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^5.0.1",
    "typescript": "2.4.1"
  },
  "peerDependencies": {
    "typescript": ">=2.1.0"
  },
  "devDependencies": {
    "@types/jest": "latest",
    "@types/node": "8.0.14",
    "jest": "20.0.4",
    "nock": "9.0.14",
    "rimraf": "^2.6.1",
    "ts-jest": "^20.0.6",
    "tslint": "5.5.0",
    "tslint-config-airbnb": "5.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "setupFiles": [
      "./loadEnv.js"
    ],
    "testRegex": "src/__tests__/.*\\.(test)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
