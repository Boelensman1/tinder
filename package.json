{
  "name": "@boelensman1/tinderclient",
  "version": "0.0.2",
  "description": "Wrapper for the tinder api",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint './src/**/*.ts' --project tsconfig.json --type-check",
    "clean": "rimraf lib",
    "prepare": "npm run lint && npm run clean && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Boelensman1/tinderclient.git"
  },
  "author": "Wigger Boelens <me@wiggerboelens.com> (http://wiggerboelens.com/)",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "typescript": "^2.7.2"
  },
  "peerDependencies": {
    "typescript": ">=2.1.0"
  },
  "devDependencies": {
    "@types/jest": "latest",
    "@types/node": "^9.6.0",
    "jest": "^22.4.3",
    "nock": "^9.2.3",
    "rimraf": "^2.6.1",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/__tests__/.*\\.(test)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
